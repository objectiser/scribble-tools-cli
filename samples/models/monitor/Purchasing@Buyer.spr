import Order;
import CreditCheck;
import CreditOk;
import InsufficientCredit;
import Confirmation;
import OutOfStock;

protocol Purchasing at Buyer (role Broker) {

	Order to Broker;
	
	choice {
		Confirmation from Broker;
	} or {
		OutOfStock from Broker;
	} or {
		InsufficientCredit from Broker;
	}
}
